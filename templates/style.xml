<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Catppuccin {{titlecase flavour}}">
  <style name="Text" foreground="#{{text}}" background="#{{base}}"/>
  <style name="Link" foreground="#{{sky}}" underlineStyle="SingleUnderline"/>
  <style name="Selection" background="#{{surface2}}"/>
  <style name="LineNumber" foreground="#{{surface1}}"/>
  <style name="SearchResult" foreground="#{{teal}}" bold="true"/>
  <style name="SearchScope" foreground="#{{base}}" background="#{{text}}"/>
  <style name="Parentheses" foreground="#{{peach}}" bold="true"/>
  <style name="ParenthesesMismatch" foreground="#{{peach}}"/>
  <style name="AutoComplete" foreground="#{{text}}"/>
  <style name="CurrentLine" foreground="#{{base}}" background="#{{surface0}}"/>
  <style name="CurrentLineNumber" foreground="#{{overlay0}}"/>
  <style name="Occurrences" foreground="#{{base}}" background="#{{text}}"/>
  <style name="Occurrences.Unused" foreground="#{{text}}"/>
  <style name="Occurrences.Rename" foreground="#{{base}}" background="#{{text}}"/>
  <style name="Number" foreground="#{{peach}}"/>
  <style name="String" foreground="#{{green}}"/>
  <style name="Type" foreground="#{{yellow}}"/>
  <style name="Local" foreground="#{{text}}"/>
  <style name="Parameter"/>
  <style name="Global" foreground="#{{maroon}}"/>
  <style name="Field" foreground="#{{rosewater}}"/>
  <style name="Static" foreground="#{{teal}}"/>
  <style name="VirtualMethod" foreground="#{{blue}}" italic="true"/>
  <style name="Function" foreground="#{{blue}}"/>
  <style name="Keyword" foreground="#{{mauve}}"/>
  <style name="PrimitiveType" foreground="#{{yellow}}" italic="true"/>
  <style name="Operator" foreground="#{{sky}}"/>
  <style name="Overloaded Operator" foreground="#{{sky}}"/>
  <style name="Punctuation"/>
  <style name="Preprocessor" foreground="#{{sky}}"/>
  <style name="Label" foreground="#{{sky}}"/>
  <style name="Comment" foreground="#{{surface2}}" italic="true"/>
  <style name="Doxygen.Comment" foreground="#{{surface2}}"/>
  <style name="Doxygen.Tag" foreground="#{{surface2}}" bold="true"/>
  <style name="VisualWhitespace" foreground="#{{surface2}}"/>
  <style name="QmlLocalId" foreground="#{{green}}" italic="true"/>
  <style name="QmlExternalId" foreground="#{{peach}}" italic="true"/>
  <style name="QmlTypeId" foreground="#{{sky}}"/>
  <style name="QmlRootObjectProperty" foreground="#{{green}}" italic="true"/>
  <style name="QmlScopeObjectProperty" foreground="#{{green}}" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#{{peach}}" italic="true"/>
  <style name="JsScopeVar" foreground="#{{blue}}" italic="true"/>
  <style name="JsImportVar" foreground="#{{text}}" italic="true"/>
  <style name="JsGlobalVar" foreground="#{{red}}" italic="true"/>
  <style name="QmlStateName" foreground="#{{green}}" italic="true"/>
  <style name="Binding" foreground="#{{pink}}"/>
  <style name="DisabledCode" foreground="#{{overlay0}}"/>
  <style name="AddedLine" foreground="#{{green}}"/>
  <style name="RemovedLine" foreground="#{{red}}"/>
  <style name="DiffFile" foreground="#{{sky}}"/>
  <style name="DiffLocation" foreground="#{{peach}}"/>
  <style name="DiffFileLine" foreground="#{{base}}" background="#{{yellow}}"/>
  <style name="DiffContextLine" foreground="#{{base}}" background="#{{sky}}"/>
  <style name="DiffSourceLine" background="#{{red2}}"/>
  <style name="DiffSourceChar" background="#{{red1}}"/>
  <style name="DiffDestLine" background="#{{green2}}"/>
  <style name="DiffDestChar" background="#{{green1}}"/>
  <style name="LogChangeLine" foreground="#{{red}}"/>
  <style name="Warning" underlineColor="#{{peach}}" underlineStyle="SingleUnderline"/>
  <style name="WarningContext" underlineColor="#{{peach}}" underlineStyle="DotLine"/>
  <style name="Error" underlineColor="#{{red}}" underlineStyle="SingleUnderline"/>
  <style name="ErrorContext" underlineColor="#{{red}}" underlineStyle="DotLine"/>
  <style name="Declaration" bold="true"/>
  <style name="FunctionDefinition"/>
  <style name="OutputArgument" italic="true"/>
  <style name="StaticMember" foreground="#{{peach}}"/>
</style-scheme>
